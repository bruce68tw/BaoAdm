<form id='eform' class='xg-form'>
    @await Component.InvokeAsync("XiHide", new XiHideDto { Fid = "Id" })

    @await Component.InvokeAsync("XiText", new XiTextDto { Title = "帳號", Fid = "Account", MaxLen = 30 })
    @await Component.InvokeAsync("XiText", new XiTextDto { Title = "姓名", Fid = "Name", MaxLen = 50 })
    @await Component.InvokeAsync("XiText", new XiTextDto { Title = "手機", Fid = "Phone", MaxLen = 15 })
    @await Component.InvokeAsync("XiText", new XiTextDto { Title = "Email", Fid = "Email", MaxLen = 100 })
    @await Component.InvokeAsync("XiCheck", new XiCheckDto { Title = "是否公司", Fid = "IsCorp", Label = "是" })
    @await Component.InvokeAsync("XiRead", new XiReadDto { Title = "建檔日期", Fid = "Created" })
</form>

@await Component.InvokeAsync("XgSaveBack")

