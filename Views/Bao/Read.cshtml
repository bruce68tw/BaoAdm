<script src="~/js/view/Bao.js"></script>
<script type="text/javascript">
    $(function () {
        _me.init();
    });
</script>

@await Component.InvokeAsync("XgProgPath", new { names = new string[] { "尋寶資料查詢" } })
<div class="xp-prog">
    <div id="divRead">
		<form id='formRead' class='xg-form xg-mb10'>
			<div class="row">
				@await Component.InvokeAsync("XiText", new XiTextDto{ Title = "尋寶名稱", Fid = "Name", MaxLen = 100, InRow = true })
				@await Component.InvokeAsync("XgFindTbar", new XgFindTbarDto{  })
			</div>
        </form>

        <table id="tableRead" class="table table-bordered xg-table" cellspacing="0">
            <thead>
                <tr>
					<th>尋寶名稱</th>
					<th>開始時間</th>
					<th>結束時間</th>
					<th>是否<br>批次解謎</th>
					<th>是否<br>移動地點</th>
					<th>獎品種類</th>
					<th>獎品內容</th>
					<th>關卡數目</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>